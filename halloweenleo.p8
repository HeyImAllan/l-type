#include entities\auditron.lua

-- main
function _init()
music(12)
setpalt()

-- game settings
mode="menu"
level=1
halloweenwave=1
scene_start=true

movementspeed=2

bullets={}

afterburner_gfx=51
afterburner_x=-20
afterburner_time=0
afterburner_time_max=150
afterburner_on=false
muzzle=-1

-- graphics
default_bg=1
frame_count=0
t=0
cls(default_bg)
score=0
fgspeed=0.25
spawn_auditron(50)

-- timing vars
nodmg_frames=0
-- wait time between shots
shoot_wait=3
-- wait time before game over is triggered.
gameoverwait=-1
winwait=-1

end

function _draw()
 cls()
 if mode == "menu" then
  update_auditrons(auditrons)
  draw_auditrons(auditrons)
  print("l-type beta 31-10-2025",18,48,12)
  print("welcome to the 2025",20,10,9)
  print("halloween game event!",20,20,9)

  print("press ‚ùé to play",28,56,menu_gfx())
  

 end
 
 if mode == "gameover" then
  draw_gameover()
 end

 if mode == "gamewon" then
  if winwait == 0 then
    music(12)
  end
  if winwait < 0 then
    draw_gamewin()
  else
    draw_l01()
    winwait-=1
    end
 end
 if mode == "game" then
  if level == 1 then
    draw_l01()
  end
 end
 frame_count+=1
 t+=1
end

function _update()
 if mode == "game" then
  if level == 1 then
   update_l01()
  end
  update_game()
 end
 if mode == "menu" then
  if btnp(5) then
   explosions={}
   leo=new_leo()
   reset_level(level)
   halloweenwave=1
   scene_start=true
   mode = "game"
   sleep=30
   shoot_wait=3
   _update()
  end
 end 

 if (mode == "gameover" or mode== "gamewon") and not ( gameoverwait > 0 or winwait > 0) then
  if btnp(5) then
    mode = "menu"
    run()
  end
 end
end
	
-- foreground animations
blinkt=0
function menu_gfx()
 local banim = {5,5,5,6,6,6,7,7,7,7,7,7,6,6,6}
 if blinkt > #banim then
  blinkt = 0
 end
 blinkt=blinkt+1
 return banim[blinkt]
end

-- draw game
function draw_generic_game()
  -- woo so many points :)
  if score != 0 then
    print("score: "..score.."000",40,1,12)
  else
    print("score: "..score,40,1,12)
  end
  for i=0,leo.max_health do
   if (i < leo.health) then
     spr(20,i*9,1)
   else
     spr(21,i*9,1)
   end
  end


  if gameoverwait == -1 or gameoverwait > 50 then
    if nodmg_frames == 0 then
    draw_leo(leo)
    else
    if frame_count % 10 != 0 then
      draw_leo(leo)
    end
    end
  end
  draw_bullets()
  draw_explosions(explosions)
  draw_shrapnel(shraps,12)
  draw_shrapnel(sparks,9)
  draw_shwaves(shwaves)
  
  -- shooting explosion
  circfill(leo.x+18,leo.y+12,muzzle,7)
  leoreset(leo)
end

function setpalt()
 palt(0,false)
 palt(15,true)
end

function reset_level(level)
  reset_l01()
end
-- update game
function update_game()
 -- leo button movement
 -- moves left
 if gameoverwait <= 0 and winwait <= 0 then
  if btn(0) then
    leo.sx=movementspeed*-1
    leo.invert_spr=true
    leo.prop_x=-8
    afterburner_x=16
  end

  -- moves right
  if btn(1) then
    leo.sx=movementspeed
    leo.prop_x=16
    afterburner_x=-7
  end

  -- moves down	
    if btn(2) then
    leo.sy=movementspeed*-1
    leo.plane_sspr_sy=24
    end
    
  -- moves up
    if btn(3) then
    leo.sy=movementspeed
    leo.plane_sspr_sy=8
    end

  --enable booster
  if btnp(4) then
    afterburner_on=true
    sfx(2)
  end		
    --leo shooting
  if btn(5) then
    shoot()
  end
end
	
 if (afterburner_on == false) then
 	prop_gfx(leo)
 else
  afterburner_processing()
 end
	
 leo_vert_bob(leo)
 leoboundscheck(leo)			
 move_leo(leo)

 // calculate bullet movements
 move_bullets()
 if gameoverwait > 0 then
  gameoverwait-=1
 end
 if gameoverwait == 0 then
  mode = "gameover"
  gameoverwait = -1
  music(10)
 end
 if nodmg_frames > 0 then
  nodmg_frames-=1
 end
end

